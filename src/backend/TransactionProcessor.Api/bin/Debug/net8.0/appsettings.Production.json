{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Host=<db-host>;Port=5432;Database=transactionprocessor;Username=<db-user>;Password=<db-pass>;Ssl Mode=Require"
  },
  "AWS": {
    "S3": {
      "BucketName": "<cnab-files-bucket>",
      "ServiceUrl": null
    },
    "SQS": {
      "QueueUrl": "https://sqs.<region>.amazonaws.com/<account-id>/file-processing-queue",
      "DlqUrl": "https://sqs.<region>.amazonaws.com/<account-id>/file-processing-dlq",
      "ServiceUrl": null,
      "VisibilityTimeoutSeconds": 300
    },
    "Cognito": {
      "Authority": "https://cognito-idp.<region>.amazonaws.com/<user-pool-id>",
      "Audience": "<api-client-id>"
    }
  },
  "CORS": {
    "AllowedOrigins": [],
    "AllowHeaders": ["*"],
    "AllowMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "AllowCredentials": true
  },
  "AllowedHosts": "*"
}
