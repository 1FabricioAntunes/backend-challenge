{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "<set-via-env-or-secrets>"
  },
  "AWS": {
    "S3": {
      "BucketName": "<cnab-files-bucket>",
      "ServiceUrl": null
    },
    "SQS": {
      "QueueUrl": "<file-processing-queue-url>",
      "DlqUrl": "<file-processing-dlq-url>",
      "ServiceUrl": null,
      "VisibilityTimeoutSeconds": 300
    },
    "Cognito": {
      "Authority": "<https://cognito-idp.<region>.amazonaws.com/<user-pool-id>>",
      "Audience": "<api-client-id>"
    }
  },
  "CORS": {
    "AllowedOrigins": [],
    "AllowHeaders": ["*"],
    "AllowMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "AllowCredentials": true
  }
}
